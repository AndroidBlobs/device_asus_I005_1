on property:vendor.asus.donglestate_GV_PD=2
	setprop vendor.asus.aura.gamepad none

# ASUS_BPS Add for AuraLight HAL control ---


on property:vendor.asus.gamingtype=*
	setprop vendor.gamingtype  ${vendor.asus.gamingtype}


on property:sys.pmoled.power=*
        start cscSecdisplay

on property:sys.pmoled.pattern=*
	write /sys/class/spi_master/spi0/spi0.0/Pattern ${sys.pmoled.pattern}

